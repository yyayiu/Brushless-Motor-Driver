$(function(){
		var replaceWidth = 768;
		var $this = $(this);
		function fixHeader(){
			var windowWidth = parseInt($(window).width());
			if(windowWidth >= replaceWidth) {
				$('body').removeClass('fixHeader');
			} else if(windowWidth < replaceWidth) {
				$('body').addClass('fixHeader');
			}
		}
		$(window).resize(function(){fixHeader();});
		fixHeader();
});



