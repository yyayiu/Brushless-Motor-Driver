(function(document, window, $) {

    $(document).ready(function(){

        var imageEnlargeApplied = false;

        /**
         * Functions
         */
        function applyImageEnlarge() {
            
            /**
             * Enlarge Icon
             */
            var enlarge = '<span class="enlarge-icon fa-stack fa-lg">'+ 
                '<i class="fa fa-square fa-stack-2x" style="color: rgba(202,202,202,.5);"></i>'+
                '<i class="fa fa-search-plus fa-stack-1x fa-inverse"></i>'+
                '</span>';

            /**
             * Wrapper for the image to be enlarged
             */
            $(".image-enlarge").wrap('<div class="image-enlarge-container"></div>');
            $(".image-enlarge").after(enlarge);
            
            /**
             * Listeners
             */
            $(".image-enlarge").on("click", function() {
                $('#imagepreview').attr('src', $(this).attr('src'));
                var title = $(this).attr('title');
                if (title != "" && title != null) {
                    $('#imagemodal #modal-title').text( title );
                }
                $('#imagemodal').modal('show');
            });

            $(".enlarge-icon").on("click", function() {
                var imgsource = $(this).parent().find("img.image-enlarge");
                console.log(imgsource);
                $('#imagepreview').attr('src', imgsource.attr('src'));
                var title = imgsource.attr('title');
                if (title != "" && title != null) {
                    $('#imagemodal #modal-title').text( title );
                }
                $('#imagemodal').modal('show');
            });

            //  If modal is for an image set size
            var $imageModal = $('.modal-dialog').find('img');
            if ($imageModal.length) {
                $('.modal-dialog').addClass('imageModal');
            }

            imageEnlargeApplied = true;
        }

        function removeImageEnlarge() {
            // code to remove image enlargement
            // this is to be invoked when mobile only
            $(".image-enlarge").unwrap();
            $(".image-enlarge").unbind("click");
            $(".enlarge-icon").unbind("click").remove();

            imageEnlargeApplied = false;
        }

        /**
         * Apply Image Enlarge for demanding images
         */
        if ( $('.image-enlarge').length && $(window).width() >= 765 ) {

            applyImageEnlarge();

            $(window).resize(function() {
                if( $(window).width() >= 765 && !imageEnlargeApplied ) {
                    applyImageEnlarge();
                }
                if( $(window).width() < 765 && imageEnlargeApplied ) {
                    removeImageEnlarge();
                }
            });
        }
    });

    console.log('loaded');

